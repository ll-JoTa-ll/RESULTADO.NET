@model IEnumerable<ENTIDADES.ENT_Sucursal>

@{
    ViewBag.Title = "Listado";
}

<h2>Listado</h2>

<p>
    @Html.ActionLink("Crear", "Registro")
</p>
<table class="table">
    <tr>
        <th>
            Nombre Sucursal
        </th>
        <th>
            Direccion Sucursal
        </th>
        <th>
            Fec. Registro
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Sucursal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Direccion_Sucursal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Registro)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id = item.Id_Sucursal }) |
            @*@Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |*@
            @Html.ActionLink("Eliminar", "Eliminar", new { id = item.Id_Sucursal })
        </td>
    </tr>
}

</table>
